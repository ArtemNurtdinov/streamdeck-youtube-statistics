{
  "name": "streamdeck-youtube-statistics-sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf com.nefrit.youtube.sdPlugin/bin",
    "build": "npm run clean && rollup -c",
    "watch": "rollup -c -w",
    "restart": "streamdeck restart com.nefrit.youtube",
    "dev": "npm run watch",
    "validate": "streamdeck validate ./com.nefrit.youtube.sdPlugin",
    "pack": "streamdeck pack ./com.nefrit.youtube.sdPlugin"
  },
  "keywords": [
    "streamdeck",
    "youtube",
    "plugin"
  ],
  "author": "ArtemNeFRiT",
  "license": "ISC",
  "description": "TypeScript SDK migration for YouTube Stats Tracker plugin.",
  "dependencies": {
    "@elgato/streamdeck": "^2.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^25.2.3",
    "rimraf": "^6.1.2",
    "rollup": "^4.52.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
