{
  "name": "streamdeck-youtube-statistics-sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf com.nefrit.youtube.sdPlugin/bin",
    "build": "npm run clean && tsc -p tsconfig.json",
    "postbuild": "node prepare-runtime.js",
    "watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "watch:reload": "tsc-watch -p tsconfig.json --onSuccess \"streamdeck restart com.nefrit.youtube\"",
    "restart": "streamdeck restart com.nefrit.youtube",
    "dev": "npm run watch:reload",
    "validate": "streamdeck validate ./com.nefrit.youtube.sdPlugin",
    "pack": "streamdeck pack ./com.nefrit.youtube.sdPlugin"
  },
  "keywords": [
    "streamdeck",
    "youtube",
    "plugin"
  ],
  "author": "ArtemNeFRiT",
  "license": "ISC",
  "description": "TypeScript SDK migration for YouTube Stats Tracker plugin.",
  "dependencies": {
    "@elgato/streamdeck": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "rimraf": "^6.1.2",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
